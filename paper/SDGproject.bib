  keywords = {policy diffusion,policy innovation,research frontier,政策创新,政策扩散,研究前沿}
}

@article{ZhuXuFengZhangYouLang2014,
  title = {{地方政府创新经验推广的难点何在\textemdash\textemdash 公共政策创新扩散理论的研究评述}},
  author = {朱旭峰 and 张友浪},
